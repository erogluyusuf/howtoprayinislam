<quran>
<sura index="93" name="الضحى">
		<aya index="1" text="بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ وَالضُّحَىٰ"/>
		<aya index="2" text="وَاللَّيْلِ إِذَا سَجَىٰ"/>
		<aya index="3" text="مَا وَدَّعَكَ رَبُّكَ وَمَا قَلَىٰ"/>
		<aya index="4" text="وَلَلْآخِرَةُ خَيْرٌ لَكَ مِنَ الْأُولَىٰ"/>
		<aya index="5" text="وَلَسَوْفَ يُعْطِيكَ رَبُّكَ فَتَرْضَىٰ"/>
		<aya index="6" text="أَلَمْ يَجِدْكَ يَتِيمًا فَآوَىٰ"/>
		<aya index="7" text="وَوَجَدَكَ ضَالًّا فَهَدَىٰ"/>
		<aya index="8" text="وَوَجَدَكَ عَائِلًا فَأَغْنَىٰ"/>
		<aya index="9" text="فَأَمَّا الْيَتِيمَ فَلَا تَقْهَرْ"/>
		<aya index="10" text="وَأَمَّا السَّائِلَ فَلَا تَنْهَرْ"/>
		<aya index="11" text="وَأَمَّا بِنِعْمَةِ رَبِّكَ فَحَدِّثْ"/>
	</sura>
<sura index="94" name="الانشراح">
		<aya index="1" text="بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ أَلَمْ نَشْرَحْ لَكَ صَدْرَكَ"/>
		<aya index="2" text="وَوَضَعْنَا عَنْكَ وِزْرَكَ"/>
		<aya index="3" text="الَّذِي أَنْقَضَ ظَهْرَكَ"/>
		<aya index="4" text="وَرَفَعْنَا لَكَ ذِكْرَكَ"/>
		<aya index="5" text="فَإِنَّ مَعَ الْعُسْرِ يُسْرًا"/>
		<aya index="6" text="إِنَّ مَعَ الْعُسْرِ يُسْرًا"/>
		<aya index="7" text="فَإِذَا فَرَغْتَ فَانْصَبْ"/>
		<aya index="8" text="وَإِلَىٰ رَبِّكَ فَارْغَبْ"/>
	</sura>
<sura index="95" name="لتي">
		<aya index="1" text="بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ وَالتِّينِ وَالزَّيْتُونِ"/>
		<aya index="2" text="وَطُورِ سِينِينَ"/>
		<aya index="3" text="وَهَٰذَا الْبَلَدِ الْأَمِينِ"/>
		<aya index="4" text="لَقَدْ خَلَقْنَا الْإِنْسَانَ فِي أَحْسَنِ تَقْوِيمٍ"/>
		<aya index="5" text="ثُمَّ رَدَدْنَاهُ أَسْفَلَ سَافِلِينَ"/>
		<aya index="6" text="إِلَّا الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ فَلَهُمْ أَجْرٌ غَيْرُ مَمْنُونٍ"/>
		<aya index="7" text="فَمَا يُكَذِّبُكَ بَعْدُ بِالدِّينِ"/>
		<aya index="8" text="أَلَيْسَ اللَّهُ بِأَحْكَمِ الْحَاكِمِينَ"/>
	</sura>
<sura index="96" name="العلق">
		<aya index="1" text="بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ اقْرَأْ بِاسْمِ رَبِّكَ الَّذِي خَلَقَ"/>
		<aya index="2" text="خَلَقَ الْإِنْسَانَ مِنْ عَلَقٍ"/>
		<aya index="3" text="اقْرَأْ وَرَبُّكَ الْأَكْرَمُ"/>
		<aya index="4" text="الَّذِي عَلَّمَ بِالْقَلَمِ"/>
		<aya index="5" text="عَلَّمَ الْإِنْسَانَ مَا لَمْ يَعْلَمْ"/>
		<aya index="6" text="كَلَّا إِنَّ الْإِنْسَانَ لَيَطْغَىٰ"/>
		<aya index="7" text="أَنْ رَآهُ اسْتَغْنَىٰ"/>
		<aya index="8" text="إِنَّ إِلَىٰ رَبِّكَ الرُّجْعَىٰ"/>
		<aya index="9" text="أَرَأَيْتَ الَّذِي يَنْهَىٰ"/>
		<aya index="10" text="عَبْدًا إِذَا صَلَّىٰ"/>
		<aya index="11" text="أَرَأَيْتَ إِنْ كَانَ عَلَى الْهُدَىٰ"/>
		<aya index="12" text="أَوْ أَمَرَ بِالتَّقْوَىٰ"/>
		<aya index="13" text="أَرَأَيْتَ إِنْ كَذَّبَ وَتَوَلَّىٰ"/>
		<aya index="14" text="أَلَمْ يَعْلَمْ بِأَنَّ اللَّهَ يَرَىٰ"/>
		<aya index="15" text="كَلَّا لَئِنْ لَمْ يَنْتَهِ لَنَسْفَعًا بِالنَّاصِيَةِ"/>
		<aya index="16" text="نَاصِيَةٍ كَاذِبَةٍ خَاطِئَةٍ"/>
		<aya index="17" text="فَلْيَدْعُ نَادِيَهُ"/>
		<aya index="18" text="سَنَدْعُ الزَّبَانِيَةَ"/>
		<aya index="19" text="كَلَّا لَا تُطِعْهُ وَاسْجُدْ وَاقْتَرِبْ ۩"/>
	</sura>
<sura index="97" name="القدر">
		<aya index="1" text="بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ إِنَّا أَنْزَلْنَاهُ فِي لَيْلَةِ الْقَدْرِ"/>
		<aya index="2" text="وَمَا أَدْرَاكَ مَا لَيْلَةُ الْقَدْرِ"/>
		<aya index="3" text="لَيْلَةُ الْقَدْرِ خَيْرٌ مِنْ أَلْفِ شَهْرٍ"/>
		<aya index="4" text="تَنَزَّلُ الْمَلَائِكَةُ وَالرُّوحُ فِيهَا بِإِذْنِ رَبِّهِمْ مِنْ كُلِّ أَمْرٍ"/>
		<aya index="5" text="سَلَامٌ هِيَ حَتَّىٰ مَطْلَعِ الْفَجْرِ"/>
	</sura>
<sura index="98" name="البينة">
		<aya index="1" text="بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ لَمْ يَكُنِ الَّذِينَ كَفَرُوا مِنْ أَهْلِ الْكِتَابِ وَالْمُشْرِكِينَ مُنْفَكِّينَ حَتَّىٰ تَأْتِيَهُمُ الْبَيِّنَةُ"/>
		<aya index="2" text="رَسُولٌ مِنَ اللَّهِ يَتْلُو صُحُفًا مُطَهَّرَةً"/>
		<aya index="3" text="فِيهَا كُتُبٌ قَيِّمَةٌ"/>
		<aya index="4" text="وَمَا تَفَرَّقَ الَّذِينَ أُوتُوا الْكِتَابَ إِلَّا مِنْ بَعْدِ مَا جَاءَتْهُمُ الْبَيِّنَةُ"/>
		<aya index="5" text="وَمَا أُمِرُوا إِلَّا لِيَعْبُدُوا اللَّهَ مُخْلِصِينَ لَهُ الدِّينَ حُنَفَاءَ وَيُقِيمُوا الصَّلَاةَ وَيُؤْتُوا الزَّكَاةَ ۚ وَذَٰلِكَ دِينُ الْقَيِّمَةِ"/>
		<aya index="6" text="إِنَّ الَّذِينَ كَفَرُوا مِنْ أَهْلِ الْكِتَابِ وَالْمُشْرِكِينَ فِي نَارِ جَهَنَّمَ خَالِدِينَ فِيهَا ۚ أُولَٰئِكَ هُمْ شَرُّ الْبَرِيَّةِ"/>
		<aya index="7" text="إِنَّ الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ أُولَٰئِكَ هُمْ خَيْرُ الْبَرِيَّةِ"/>
		<aya index="8" text="جَزَاؤُهُمْ عِنْدَ رَبِّهِمْ جَنَّاتُ عَدْنٍ تَجْرِي مِنْ تَحْتِهَا الْأَنْهَارُ خَالِدِينَ فِيهَا أَبَدًا ۖ رَضِيَ اللَّهُ عَنْهُمْ وَرَضُوا عَنْهُ ۚ ذَٰلِكَ لِمَنْ خَشِيَ رَبَّهُ"/>
	</sura>
<sura index="99" name="الزلزلة">
		<aya index="1" text="بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ إِذَا زُلْزِلَتِ الْأَرْضُ زِلْزَالَهَا"/>
		<aya index="2" text="وَأَخْرَجَتِ الْأَرْضُ أَثْقَالَهَا"/>
		<aya index="3" text="وَقَالَ الْإِنْسَانُ مَا لَهَا"/>
		<aya index="4" text="يَوْمَئِذٍ تُحَدِّثُ أَخْبَارَهَا"/>
		<aya index="5" text="بِأَنَّ رَبَّكَ أَوْحَىٰ لَهَا"/>
		<aya index="6" text="يَوْمَئِذٍ يَصْدُرُ النَّاسُ أَشْتَاتًا لِيُرَوْا أَعْمَالَهُمْ"/>
		<aya index="7" text="فَمَنْ يَعْمَلْ مِثْقَالَ ذَرَّةٍ خَيْرًا يَرَهُ"/>
		<aya index="8" text="وَمَنْ يَعْمَلْ مِثْقَالَ ذَرَّةٍ شَرًّا يَرَهُ"/>
	</sura>
<sura index="100" name="العاديات">
		<aya index="1" text="بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ وَالْعَادِيَاتِ ضَبْحًا"/>
		<aya index="2" text="فَالْمُورِيَاتِ قَدْحًا"/>
		<aya index="3" text="فَالْمُغِيرَاتِ صُبْحًا"/>
		<aya index="4" text="فَأَثَرْنَ بِهِ نَقْعًا"/>
		<aya index="5" text="فَوَسَطْنَ بِهِ جَمْعًا"/>
		<aya index="6" text="إِنَّ الْإِنْسَانَ لِرَبِّهِ لَكَنُودٌ"/>
		<aya index="7" text="وَإِنَّهُ عَلَىٰ ذَٰلِكَ لَشَهِيدٌ"/>
		<aya index="8" text="وَإِنَّهُ لِحُبِّ الْخَيْرِ لَشَدِيدٌ"/>
		<aya index="9" text="أَفَلَا يَعْلَمُ إِذَا بُعْثِرَ مَا فِي الْقُبُورِ"/>
		<aya index="10" text="وَحُصِّلَ مَا فِي الصُّدُورِ"/>
		<aya index="11" text="إِنَّ رَبَّهُمْ بِهِمْ يَوْمَئِذٍ لَخَبِيرٌ"/>
	</sura>
<sura index="101" name="القارعة">
		<aya index="1" text="بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ الْقَارِعَةُ"/>
		<aya index="2" text="مَا الْقَارِعَةُ"/>
		<aya index="3" text="وَمَا أَدْرَاكَ مَا الْقَارِعَةُ"/>
		<aya index="4" text="يَوْمَ يَكُونُ النَّاسُ كَالْفَرَاشِ الْمَبْثُوثِ"/>
		<aya index="5" text="وَتَكُونُ الْجِبَالُ كَالْعِهْنِ الْمَنْفُوشِ"/>
		<aya index="6" text="فَأَمَّا مَنْ ثَقُلَتْ مَوَازِينُهُ"/>
		<aya index="7" text="فَهُوَ فِي عِيشَةٍ رَاضِيَةٍ"/>
		<aya index="8" text="وَأَمَّا مَنْ خَفَّتْ مَوَازِينُهُ"/>
		<aya index="9" text="فَأُمُّهُ هَاوِيَةٌ"/>
		<aya index="10" text="وَمَا أَدْرَاكَ مَا هِيَهْ"/>
		<aya index="11" text="نَارٌ حَامِيَةٌ"/>
	</sura>
<sura index="102" name="التكاثر">
		<aya index="1" text="بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ أَلْهَاكُمُ التَّكَاثُرُ"/>
		<aya index="2" text="حَتَّىٰ زُرْتُمُ الْمَقَابِرَ"/>
		<aya index="3" text="كَلَّا سَوْفَ تَعْلَمُونَ"/>
		<aya index="4" text="ثُمَّ كَلَّا سَوْفَ تَعْلَمُونَ"/>
		<aya index="5" text="كَلَّا لَوْ تَعْلَمُونَ عِلْمَ الْيَقِينِ"/>
		<aya index="6" text="لَتَرَوُنَّ الْجَحِيمَ"/>
		<aya index="7" text="ثُمَّ لَتَرَوُنَّهَا عَيْنَ الْيَقِينِ"/>
		<aya index="8" text="ثُمَّ لَتُسْأَلُنَّ يَوْمَئِذٍ عَنِ النَّعِيمِ"/>
	</sura>
<sura index="103" name="العصر">
		<aya index="1" text="بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ وَالْعَصْرِ"/>
		<aya index="2" text="إِنَّ الْإِنْسَانَ لَفِي خُسْرٍ"/>
		<aya index="3" text="إِلَّا الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ وَتَوَاصَوْا بِالْحَقِّ وَتَوَاصَوْا بِالصَّبْرِ"/>
	</sura>
<sura index="104" name="الهمزة">
		<aya index="1" text="بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ وَيْلٌ لِكُلِّ هُمَزَةٍ لُمَزَةٍ"/>
		<aya index="2" text="الَّذِي جَمَعَ مَالًا وَعَدَّدَهُ"/>
		<aya index="3" text="يَحْسَبُ أَنَّ مَالَهُ أَخْلَدَهُ"/>
		<aya index="4" text="كَلَّا ۖ لَيُنْبَذَنَّ فِي الْحُطَمَةِ"/>
		<aya index="5" text="وَمَا أَدْرَاكَ مَا الْحُطَمَةُ"/>
		<aya index="6" text="نَارُ اللَّهِ الْمُوقَدَةُ"/>
		<aya index="7" text="الَّتِي تَطَّلِعُ عَلَى الْأَفْئِدَةِ"/>
		<aya index="8" text="إِنَّهَا عَلَيْهِمْ مُؤْصَدَةٌ"/>
		<aya index="9" text="فِي عَمَدٍ مُمَدَّدَةٍ"/>
	</sura>
<sura index="105" name="الفيل">
		<aya index="1" text="بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ أَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِأَصْحَابِ الْفِيلِ"/>
		<aya index="2" text="أَلَمْ يَجْعَلْ كَيْدَهُمْ فِي تَضْلِيلٍ"/>
		<aya index="3" text="وَأَرْسَلَ عَلَيْهِمْ طَيْرًا أَبَابِيلَ"/>
		<aya index="4" text="تَرْمِيهِمْ بِحِجَارَةٍ مِنْ سِجِّيلٍ"/>
		<aya index="5" text="فَجَعَلَهُمْ كَعَصْفٍ مَأْكُولٍ"/>
	</sura>
<sura index="106" name="قريش">
		<aya index="1" text="بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ لِإِيلَافِ قُرَيْشٍ"/>
		<aya index="2" text="إِيلَافِهِمْ رِحْلَةَ الشِّتَاءِ وَالصَّيْفِ"/>
		<aya index="3" text="فَلْيَعْبُدُوا رَبَّ هَٰذَا الْبَيْتِ"/>
		<aya index="4" text="الَّذِي أَطْعَمَهُمْ مِنْ جُوعٍ وَآمَنَهُمْ مِنْ خَوْفٍ"/>
	</sura>
<sura index="107" name="الماعون">
		<aya index="1" text="بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ أَرَأَيْتَ الَّذِي يُكَذِّبُ بِالدِّينِ"/>
		<aya index="2" text="فَذَٰلِكَ الَّذِي يَدُعُّ الْيَتِيمَ"/>
		<aya index="3" text="وَلَا يَحُضُّ عَلَىٰ طَعَامِ الْمِسْكِينِ"/>
		<aya index="4" text="فَوَيْلٌ لِلْمُصَلِّينَ"/>
		<aya index="5" text="الَّذِينَ هُمْ عَنْ صَلَاتِهِمْ سَاهُونَ"/>
		<aya index="6" text="الَّذِينَ هُمْ يُرَاءُونَ"/>
		<aya index="7" text="وَيَمْنَعُونَ الْمَاعُونَ"/>
	</sura>
<sura index="108" name="لكوث">
		<aya index="1" text="بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ إِنَّا أَعْطَيْنَاكَ الْكَوْثَرَ"/>
		<aya index="2" text="فَصَلِّ لِرَبِّكَ وَانْحَرْ"/>
		<aya index="3" text="إِنَّ شَانِئَكَ هُوَ الْأَبْتَرُ"/>
	</sura>
<sura index="109" name="الكافرون">
		<aya index="1" text="بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ قُلْ يَا أَيُّهَا الْكَافِرُونَ"/>
		<aya index="2" text="لَا أَعْبُدُ مَا تَعْبُدُونَ"/>
		<aya index="3" text="وَلَا أَنْتُمْ عَابِدُونَ مَا أَعْبُدُ"/>
		<aya index="4" text="وَلَا أَنَا عَابِدٌ مَا عَبَدْتُمْ"/>
		<aya index="5" text="وَلَا أَنْتُمْ عَابِدُونَ مَا أَعْبُدُ"/>
		<aya index="6" text="لَكُمْ دِينُكُمْ وَلِيَ دِينِ"/>
	</sura>
<sura index="110" name="النصر">
		<aya index="1" text="بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ إِذَا جَاءَ نَصْرُ اللَّهِ وَالْفَتْحُ"/>
		<aya index="2" text="وَرَأَيْتَ النَّاسَ يَدْخُلُونَ فِي دِينِ اللَّهِ أَفْوَاجًا"/>
		<aya index="3" text="فَسَبِّحْ بِحَمْدِ رَبِّكَ وَاسْتَغْفِرْهُ ۚ إِنَّهُ كَانَ تَوَّابًا"/>
	</sura>
<sura index="111" name="لمس">
		<aya index="1" text="بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ تَبَّتْ يَدَا أَبِي لَهَبٍ وَتَبَّ"/>
		<aya index="2" text="مَا أَغْنَىٰ عَنْهُ مَالُهُ وَمَا كَسَبَ"/>
		<aya index="3" text="سَيَصْلَىٰ نَارًا ذَاتَ لَهَبٍ"/>
		<aya index="4" text="وَامْرَأَتُهُ حَمَّالَةَ الْحَطَبِ"/>
		<aya index="5" text="فِي جِيدِهَا حَبْلٌ مِنْ مَسَدٍ"/>
	</sura>
<sura index="112" name="الاخلاص">
		<aya index="1" text="بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ قُلْ هُوَ اللَّهُ أَحَدٌ"/>
		<aya index="2" text="اللَّهُ الصَّمَدُ"/>
		<aya index="3" text="لَمْ يَلِدْ وَلَمْ يُولَدْ"/>
		<aya index="4" text="وَلَمْ يَكُنْ لَهُ كُفُوًا أَحَدٌ"/>
	</sura>
<sura index="113" name="الفلق">
		<aya index="1" text="بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ قُلْ أَعُوذُ بِرَبِّ الْفَلَقِ"/>
		<aya index="2" text="مِنْ شَرِّ مَا خَلَقَ"/>
		<aya index="3" text="وَمِنْ شَرِّ غَاسِقٍ إِذَا وَقَبَ"/>
		<aya index="4" text="وَمِنْ شَرِّ النَّفَّاثَاتِ فِي الْعُقَدِ"/>
		<aya index="5" text="وَمِنْ شَرِّ حَاسِدٍ إِذَا حَسَدَ"/>
	</sura>
<sura index="114" name="الناس">
		<aya index="1" text="بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ قُلْ أَعُوذُ بِرَبِّ النَّاسِ"/>
		<aya index="2" text="مَلِكِ النَّاسِ"/>
		<aya index="3" text="إِلَٰهِ النَّاسِ"/>
		<aya index="4" text="مِنْ شَرِّ الْوَسْوَاسِ الْخَنَّاسِ"/>
		<aya index="5" text="الَّذِي يُوَسْوِسُ فِي صُدُورِ النَّاسِ"/>
		<aya index="6" text="مِنَ الْجِنَّةِ وَالنَّاسِ"/>
	</sura>
